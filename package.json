    "css-loader": "^0.23.1",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "open": "0.0.5",
    "power-assert": "^1.4.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },

{
  "name": "respass",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "compile:ts": "tsc",
    "build:frontend": "cd ui && webpack",
    "watch:frontend": "cd ui && webpack-dev-server -d",
    "open-browser": "node -e 'setTimeout(function(){require(`open`)(`http://localhost:23000/webpack-dev-server/`)}, 2000)'",
    "start:frontend": "npm-run-all --parallel open-browser watch:frontend",
    "start:server": "npm run compile:ts && node ./lib/server/index.js",
    "start": "npm-run-all --parallel start:server start:frontend",
    "pretest": "npm run compile:ts",
    "test": "mocha lib/test"
  },
  "author": "minamorl",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.10.4",
